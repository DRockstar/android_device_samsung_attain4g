# mount     fstype  device                  [device2]   [fsoptions]

/boot       bml     boot
/cache      rfs     /dev/block/mmcblk0p4    NULL        rw,nosuid,nodev,check=no
/data       rfs     /dev/block/mmcblk0p1    NULL        rw,nosuid,nodev,check=no
/datadata   rfs     /dev/block/stl11        NULL        rw,nosuid,nodev,check=no
/recovery   bml     recovery
/sdcard     vfat    /dev/block/mmcblk1p1
/system     rfs     /dev/block/stl10        NULL        rw,check=no
